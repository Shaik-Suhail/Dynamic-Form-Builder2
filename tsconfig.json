{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["dom", "esnext"],
    "module": "ES6",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "outDir": "./dist",
    "compilerOptions": {
    "module": "es2020",
    
  }
  },
  "include": [
    "src/**/*"
  ]
}



